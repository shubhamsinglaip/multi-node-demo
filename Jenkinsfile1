pipeline {
    agent any
    
    
    stages {
        stage('stage 1') {
        agent {
            label 'jennode'
        }
            steps {
                stash 'source'
                sh 'echo "multi-node-demo"'
            }
        }
        stage('stage 2') {
        agent {
            label 'myfirstagent'
        }
            steps {
                unstage 'source'
                sh 'echo "multi-node-demo"'
            }
        }
    }
}
